<% content_for :header do %>
  <%= index_header "Customize Library-level Settings" %>
<% end %>
<%= form_with(model: @current_library, url:  admin_manage_feature_path(@current_library.id), builder: SpectreFormBuilder) do |form| %>

  <%= form.check_box :allow_members, hint: "Use to allow member creation"%>

  <%= form.actions do %>
    <%= form.submit %>
  <% end %>
<% end %>
